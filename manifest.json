{
	"manifest_version": 2,
	"name": "Trimly",
	"description": "This extension allows you to easily summarize news articles with additional relevant information.",
	"version": "1.0",
	"web_accessible_resources": ["src/options/options.html"],
	"content_security_policy": "script-src 'self' https://connect.facebook.net; object-src 'self'",
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Trim me!"
		},
	"permissions": [
	"activeTab",
	"tabs",
	"https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js",
	"https://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js",
	"storage"
	],
	"options_page": "options.html",
	"icons": {
		"16": "icons/trimly16.png",
		"128": "icons/trimly128.png"
	}
}